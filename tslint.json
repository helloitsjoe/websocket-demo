{
    "rules": {
        "curly": true,
        "triple-equals": true,
        "semicolon": [true, "always"],
        "no-var-keyword": true,
        "no-unused-expression": true,
        "no-empty": true,
        "jsdoc-format": true,
        "no-duplicate-variable": true,
        "no-debugger": true,
        "member-ordering": [
            true,
            "public-before-private",
            "static-before-instance",
            "variables-before-functions"
        ]
    }
}
